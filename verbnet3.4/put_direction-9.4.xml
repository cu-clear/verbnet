  <MEMBER fn_mapping="Body_movement" grouping="lift.01" name="lift" verbnet_key="lift#1" wn="lift%2:38:00 lift%2:35:00 lift%2:35:01" features=""/>
  <MEMBER fn_mapping="None" grouping="lower.01" name="lower" verbnet_key="lower#2" wn="lower%2:38:00" features=""/>
  <MEMBER fn_mapping="Building" grouping="raise.02" name="raise" verbnet_key="raise#5" wn="raise%2:38:00" features=""/>
  <MEMBER fn_mapping="Self_motion" grouping="" name="back" verbnet_key="back#1" wn="back%2:38:01" features=""/>
 </MEMBERS>
 <THEMROLES>
  <!--Destination can also be realized as a locative adverb ("here, there")
                                Levin doesn't include Instrument or Instrument Subject Alternation ("the crane lifted the beam over the building")-->
  <THEMROLE type="Agent">
   <SELRESTRS>
    <SELRESTR Value="+" type="animate"/>
   </SELRESTRS>
  </THEMROLE>
  <THEMROLE type="Theme">
   <SELRESTRS>
    <SELRESTR Value="+" type="concrete"/>
   </SELRESTRS>
  </THEMROLE>
  <THEMROLE type="Initial_Location">
   <SELRESTRS>
    <SELRESTR Value="+" type="location"/>
   </SELRESTRS>
  </THEMROLE>
  <THEMROLE type="Destination">
   <SELRESTRS>
    <SELRESTR Value="+" type="location"/>
    <SELRESTR Value="-" type="region"/>
   </SELRESTRS>
  </THEMROLE>
  <!--Exception: If Destination is object of a source-type preposition ("from"), then +region is allowed-->
 </THEMROLES>
 <FRAMES>
  <FRAME>
   <DESCRIPTION descriptionNumber="0.2" primary="NP V NP" secondary="Basic Transitive" xtag="0.2"/>
   <EXAMPLES>
    <EXAMPLE>
     I lifted the books.
    </EXAMPLE>
   </EXAMPLES>
   <SYNTAX>
    <NP value="Agent">
     <SELRESTRS/>
    </NP>
    <VERB/>
    <NP value="Theme">
     <SELRESTRS/>
    </NP>
   </SYNTAX>
   <SEMANTICS>
    <PRED value="has_location">
     <ARGS>
      <ARG type="Event" value="e1"/>
      <ARG type="ThemRole" value="Theme "/>
      <ARG type="ThemRole" value="?Initial_Location"/>
     </ARGS>
    </PRED>
    <PRED value="exert_force">
     <ARGS>
      <ARG type="Event" value="e2"/>
      <ARG type="ThemRole" value="Agent"/>
      <ARG type="ThemRole" value="Theme"/>
     </ARGS>
    </PRED>
    <PRED value="motion">
     <ARGS>
      <ARG type="Event" value="ë3"/>
      <ARG type="ThemRole" value="Theme"/>
      <ARG type="VerbSpecific" value="V_Trajectory"/>
