  <MEMBER fn_mapping="Change_posture" grouping="drop.01 drop.02         drop.04" name="drop" verbnet_key="drop#4" wn="drop%2:38:01" features=""/>
  <MEMBER fn_mapping="None" grouping="drive.05" name="drive" verbnet_key="drive#4" wn="drive%2:35:06" features=""/>
  <MEMBER fn_mapping="None" grouping="elevate.02 elevate.04" name="elevate" verbnet_key="elevate#1" wn="elevate%2:38:00" features=""/>
  <MEMBER fn_mapping="None" grouping="heel.01" name="heel" verbnet_key="heel#3" wn="heel%2:38:01" features=""/>
