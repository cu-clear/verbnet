  <MEMBER fn_mapping="None" grouping="recline.01" name="recline" verbnet_key="recline#1" wn="recline%2:38:00 recline%2:35:00" features=""/>
  <MEMBER fn_mapping="State_continue" grouping="rest.02" name="rest" verbnet_key="rest#1" wn="rest%2:35:04 rest%2:35:01 rest%2:35:02" features=""/>
  <MEMBER fn_mapping="Change_posture" grouping="sit.01 sit.02" name="sit" verbnet_key="sit#2" wn="sit%2:35:00 sit%2:42:00 sit%2:38:00 sit%2:35:01" features=""/>
  <MEMBER fn_mapping="None" grouping="" name="slope" verbnet_key="slope#1" wn="slope%2:38:00" features=""/>
  <MEMBER fn_mapping="None" grouping="" name="sprawl" verbnet_key="sprawl#2" wn="sprawl%2:35:00" features=""/>
  <MEMBER fn_mapping="Change_posture" grouping="stand.01 stand.02" name="stand" verbnet_key="stand#4" wn="stand%2:42:03 stand%2:35:01" features=""/>
  <MEMBER fn_mapping="None" grouping="straddle.01" name="straddle" verbnet_key="straddle#3" wn="straddle%2:42:01" features=""/>
  <MEMBER fn_mapping="None" grouping="suspend.01" name="suspend" verbnet_key="suspend#2" wn="suspend%2:35:00" features=""/>
