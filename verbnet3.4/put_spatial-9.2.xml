<VNCLASS ID="put_spatial-9.2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="vn_schema-3.xsd">
 <MEMBERS>
  <!--members specify end spatial configuration (includes orientation, support, position- intrinsic spatial properties) in has_configuration predicate-->
  <!--All members from spatial_configuration subclass have been added here as well because they take the causative change of location + change of configuration frames as well as the causative change of configuration without change of location frames, Julia 2019-->
  <MEMBER fn_mapping="None" grouping="balance.01" name="balance" verbnet_key="balance#2" wn="balance%2:42:00 balance%2:42:01" features=""/>
  <MEMBER fn_mapping="None" grouping="bend.02 bend.05" name="bend" verbnet_key="bend#5" wn="bend%2:38:00 bend%2:35:00" features=""/>
  <MEMBER fn_mapping="None" grouping="" name="dangle" verbnet_key="dangle#1" wn="dangle%2:42:00 dangle%2:35:00" features=""/>
  <MEMBER fn_mapping="None" grouping="droop.01" name="droop" verbnet_key="droop#1" wn="" features=""/>
  <MEMBER fn_mapping="None" grouping="flop.01" name="flop" verbnet_key="flop#2" wn="" features=""/>
  <MEMBER fn_mapping="None" grouping="fly.01" name="fly" verbnet_key="fly#3" wn="fly%2:39:00" features=""/>
  <MEMBER fn_mapping="Filling" grouping="hang.01" name="hang" verbnet_key="hang#4" wn="hang%2:35:03 hang%2:35:00 hang%2:42:01 hang%2:36:00 hang%2:35:04 hang%2:30:00" features=""/>
