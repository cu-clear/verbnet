  <MEMBER fn_mapping="None" grouping="open.01" name="open" verbnet_key="open#5" wn="open%2:35:00" features=""/>
  <MEMBER fn_mapping="Placing" grouping="" name="perch" verbnet_key="perch#2" wn="perch%2:35:00 perch%2:35:10" features=""/>
  <MEMBER fn_mapping="None" grouping="plop.01" name="plop" verbnet_key="plop#3" wn="plop%2:35:00" features=""/>
  <MEMBER fn_mapping="None" grouping="prop.01" name="prop" verbnet_key="prop#1" wn="prop%2:35:00" features=""/>
