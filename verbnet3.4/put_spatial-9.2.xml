 </MEMBERS>
 <THEMROLES>
  <!--Destination can also be realized as a locative adverb ("here, there")-->
  <THEMROLE type="Agent">
   <SELRESTRS>
    <SELRESTR Value="+" type="animate"/>
   </SELRESTRS>
  </THEMROLE>
  <THEMROLE type="Theme">
   <SELRESTRS>
    <SELRESTR Value="+" type="concrete"/>
   </SELRESTRS>
  </THEMROLE>
  <THEMROLE type="Destination">
   <SELRESTRS>
    <SELRESTR Value="+" type="location"/>
    <SELRESTR Value="-" type="region"/>
   </SELRESTRS>
  </THEMROLE>
 </THEMROLES>
 <FRAMES>
  <FRAME>
   <DESCRIPTION descriptionNumber="0.2" primary="NP V NP PP.destination" secondary="NP-PP; locative-PP" xtag=""/>
   <!-- many verbs cross-listed with spatial_configuration-47.6, where the change in-->
   <!-- position of Theme is emphasized; here we emphasize change in Destination-->
   <EXAMPLES>
    <EXAMPLE>
     Cheryl stood the books on the shelf.
    </EXAMPLE>
   </EXAMPLES>
   <SYNTAX>
    <NP value="Agent">
     <SELRESTRS/>
    </NP>
    <VERB/>
    <NP value="Theme">
     <SELRESTRS/>
    </NP>
    <PREP>
     <SELRESTRS>
      <SELRESTR Value="+" type="loc"/>
     </SELRESTRS>
    </PREP>
    <NP value="Destination">
     <SELRESTRS/>
    </NP>
   </SYNTAX>
   <SEMANTICS>
    <PRED value="has_location">
     <ARGS>
      <ARG type="Event" value="e1"/>
      <ARG type="ThemRole" value="Theme "/>
      <ARG type="PredSpecific" value="Initial_Location"/>
     </ARGS>
    </PRED>
    <PRED value="do">
     <ARGS>
      <ARG type="Event" value="e2"/>
      <ARG type="ThemRole" value="Agent"/>
     </ARGS>
    </PRED>
    <PRED value="motion">
     <ARGS>
      <ARG type="Event" value="ë3"/>
      <ARG type="ThemRole" value="Theme"/>
      <ARG type="PredSpecific" value="Trajectory"/>
     </ARGS>
    </PRED>
    <PRED bool="!" value="has_location">
     <ARGS>
      <ARG type="Event" value="ë3"/>
      <ARG type="ThemRole" value="Theme"/>
      <ARG type="PredSpecific" value="Initial_Location"/>
     </ARGS>
    </PRED>
    <PRED value="has_location">
     <ARGS>
      <ARG type="Event" value="e4"/>
      <ARG type="ThemRole" value="Theme"/>
      <ARG type="ThemRole" value="Destination"/>
     </ARGS>
    </PRED>
    <PRED value="has_configuration">
     <ARGS>
      <ARG type="Event" value="e4"/>
      <ARG type="ThemRole" value="Theme"/>
      <ARG type="VerbSpecific" value="V_Configuration"/>
     </ARGS>
    </PRED>
    <PRED value="cause">
     <ARGS>
      <ARG type="Event" value="e2"/>
      <ARG type="Event" value="ë3"/>
     </ARGS>
    </PRED>
   </SEMANTICS>
  </FRAME>
  <FRAME>
   <DESCRIPTION descriptionNumber="0.2" primary="NP V NP ADVP" secondary="NP-ADVP-PRED; here/there" xtag=""/>
   <!-- discourse has to provide specific Destination and configuration (Prep) of Theme wrt Destination-->
   <EXAMPLES>
    <EXAMPLE>
     Cheryl stood the books there.
    </EXAMPLE>
   </EXAMPLES>
   <SYNTAX>
    <NP value="Agent">
     <SELRESTRS/>
    </NP>
    <VERB/>
    <NP value="Theme">
     <SELRESTRS/>
    </NP>
    <NP value="Destination">
     <SELRESTRS>
      <SELRESTR Value="+" type="adv_loc"/>
     </SELRESTRS>
    </NP>
   </SYNTAX>
   <SEMANTICS>
    <PRED value="has_location">
     <ARGS>
      <ARG type="Event" value="e1"/>
      <ARG type="ThemRole" value="Theme "/>
      <ARG type="PredSpecific" value="Initial_Location"/>
     </ARGS>
    </PRED>
    <PRED value="do">
     <ARGS>
      <ARG type="Event" value="e2"/>
      <ARG type="ThemRole" value="Agent"/>
     </ARGS>
    </PRED>
    <PRED value="motion">
     <ARGS>
      <ARG type="Event" value="ë3"/>
      <ARG type="ThemRole" value="Theme"/>
      <ARG type="PredSpecific" value="Trajectory"/>
     </ARGS>
    </PRED>
    <PRED bool="!" value="has_location">
     <ARGS>
      <ARG type="Event" value="ë3"/>
      <ARG type="ThemRole" value="Theme"/>
      <ARG type="PredSpecific" value="Initial_Location"/>
     </ARGS>
    </PRED>
    <PRED value="has_location">
     <ARGS>
      <ARG type="Event" value="e4"/>
      <ARG type="ThemRole" value="Theme"/>
      <ARG type="ThemRole" value="Destination"/>
     </ARGS>
    </PRED>
    <PRED value="has_configuration">
     <ARGS>
      <ARG type="Event" value="e4"/>
      <ARG type="ThemRole" value="Theme"/>
      <ARG type="VerbSpecific" value="V_Configuration"/>
     </ARGS>
    </PRED>
    <PRED value="cause">
     <ARGS>
      <ARG type="Event" value="e2"/>
      <ARG type="Event" value="ë3"/>
     </ARGS>
    </PRED>
   </SEMANTICS>
  </FRAME>
 </FRAMES>
 <SUBCLASSES/>
</VNCLASS>