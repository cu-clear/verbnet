      <ARG type="Event" value="during(E)"/>
      <ARG type="ThemRole" value="Agent"/>
      <ARG type="ThemRole" value="?Recipient"/>
      <ARG type="ThemRole" value="Theme"/>
     </ARGS>
    </PRED>
   </SEMANTICS>
   <GL>
    <QUALIA>
     { formal = motion(e) AND Opposition(At(2, ?), At(2, -?))}
    </QUALIA>
    <EVENT_STRUCTURE>
     { var = e, initial_state = [{ objects.2.location = ? }], final_state = [{ objects.2.location = -? }] }
    </EVENT_STRUCTURE>
    <SUBCATEGORISATION>
     { var = 0, cat = NP, role = [u'Agent'] } / [u'+', u'animate'] 
	{ var = e, cat = VERB, role = [] } / [] 
	{ var = 1, cat = NP, role = [u'Patient'] } / [u'+', u'body_part'] 
	{ var = None, cat = PREP, role = [u'in'] } / [] 
	{ var = 2, cat = NP, role = [u'Theme'] } / [u'+', u'communication']
    </SUBCATEGORISATION>
   </GL>
  </FRAME>
 </FRAMES>
