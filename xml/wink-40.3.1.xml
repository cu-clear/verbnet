<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE VNCLASS SYSTEM "vn_class-3.dtd">
<VNCLASS ID="wink-40.3.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="vn_schema-3.xsd">
 <MEMBERS>
  <!--Each verb adds a manner predicate indicating how the Patient is moved by the Agent-->
  <MEMBER grouping="" name="wag" wn="wag%2:38:00"/>
 </MEMBERS>
 <THEMROLES>
  <THEMROLE type="Agent">
   <SELRESTRS>
    <SELRESTR Value="+" type="animate"/>
   </SELRESTRS>
  </THEMROLE>
  <THEMROLE type="Patient">
   <SELRESTRS>
    <SELRESTR Value="+" type="body_part"/>
   </SELRESTRS>
  </THEMROLE>
  <THEMROLE type="Theme">
   <SELRESTRS>
    <SELRESTR Value="+" type="communication"/>
   </SELRESTRS>
  </THEMROLE>
  <THEMROLE type="Recipient">
   <SELRESTRS>
    <SELRESTR Value="+" type="animate"/>
   </SELRESTRS>
  </THEMROLE>
  <!--Each verb specifies what body part is the Patient, even when not overt-->
 </THEMROLES>
 <FRAMES>
  <FRAME>
   <DESCRIPTION descriptionNumber="0.2" primary="NP V NP" secondary="Basic Transitive; Body-part Object" xtag="0.2"/>
   <!-- Patient must be a hyponym of the body part allowed by the verb, or must specify additional information-->
   <EXAMPLES>
    <EXAMPLE>
     Linda winked her eye.
    </EXAMPLE>
   </EXAMPLES>
   <SYNTAX>
    <NP value="Agent">
     <SYNRESTRS/>
    </NP>
    <VERB/>
    <NP value="Patient">
     <SYNRESTRS/>
    </NP>
   </SYNTAX>
   <SEMANTICS>
    <PRED value="cause">
     <ARGS>
      <ARG type="ThemRole" value="Agent"/>
      <ARG type="Event" value="E"/>
     </ARGS>
    </PRED>
    <PRED value="motion">
     <ARGS>
      <ARG type="Event" value="during(E)"/>
      <ARG type="ThemRole" value="Patient"/>
     </ARGS>
    </PRED>
    <PRED value="transfer_info">
     <ARGS>
      <ARG type="Event" value="during(E)"/>
      <ARG type="ThemRole" value="Agent"/>
      <ARG type="ThemRole" value="?Recipient"/>
      <ARG type="ThemRole" value="?Theme"/>
     </ARGS>
    </PRED>
   </SEMANTICS>
   <GL>
    <QUALIA>
     { formal = motion(e) AND Opposition(At(?, ?), At(?, -?))}
    </QUALIA>
    <EVENT_STRUCTURE>
     { var = e, initial_state = [{ objects.?.location = ? }], final_state = [{ objects.?.location = -? }] }
    </EVENT_STRUCTURE>
    <SUBCATEGORISATION>
     { var = 0, cat = NP, role = [u'Agent'] } / [u'+', u'animate'] 
	{ var = e, cat = VERB, role = [] } / [] 
	{ var = 1, cat = NP, role = [u'Patient'] } / [u'+', u'body_part']
    </SUBCATEGORISATION>
   </GL>
  </FRAME>
  <FRAME>
   <DESCRIPTION descriptionNumber="0.2" primary="NP V NP PP.theme" secondary="NP-PP; Theme-PP" xtag=""/>
   <EXAMPLES>
    <EXAMPLE>
     Linda winked her eye in agreement.
    </EXAMPLE>
   </EXAMPLES>
   <SYNTAX>
    <NP value="Agent">
     <SYNRESTRS/>
    </NP>
    <VERB/>
    <NP value="Patient">
     <SYNRESTRS/>
    </NP>
    <PREP value="in">
     <SELRESTRS/>
    </PREP>
    <NP value="Theme">
     <SYNRESTRS/>
    </NP>
   </SYNTAX>
   <SEMANTICS>
    <PRED value="cause">
     <ARGS>
      <ARG type="ThemRole" value="Agent"/>
      <ARG type="Event" value="E"/>
     </ARGS>
    </PRED>
    <PRED value="motion">
